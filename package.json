{
  "name": "ecars-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "bunx --bun vite",
    "build": "tsc && vite build",
    "build:dev": "tsc && vite build --mode development",
    "preview": "vite preview",
    "test": "jest",
    "test:coverage": "jest --coverage .",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "prettier": "prettier src --ext .js,.jsx,.ts,.tsx",
    "prettier:write": "prettier --write src --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@types/node": "^22.10.1",
    "axios": "^1.7.8",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.16.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.1",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "vite": "^6.0.1"
  }
}
